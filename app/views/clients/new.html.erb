<%= link_to "back", club_path(@club) %>

<%= simple_form_for [ @club, @client ] do |f| %>
  <%= f.input :first_name %>
  <%= f.input :last_name %>
  <%= f.input :email %>
  <%= f.input :phone_number %>
  <%= f.input :address %>
  <%= f.input :free_text %>
  <%= f.submit "Confirmer", class:"btn"%>
<% end %>

<%= @club.background_image %>
